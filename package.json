{
  "name": "cyclejs-examples",
  "version": "0.9.1",
  "scripts": {
    "start": "babel-node backend/scripts/app.js",
    "nodemon": "nodemon backend/scripts/app.js",
    "devel": "gulp dist & npm run nodemon & gulp watch",
    "release-patch": "git checkout master && gulp dist && git commit -am 'Distribute' && npm version patch && git push origin master --tags",
    "release-minor": "git checkout master && gulp dist && git commit -am 'Distribute' && npm version minor && git push origin master --tags",
    "release-major": "git checkout master && gulp dist && git commit -am 'Distribute' && npm version major && git push origin master --tags"
  },
  "dependencies": {
    "babel": "^4.7.16",
    "body-parser": "^1.12.4",
    "bower": "^1.3.12",
    "classnames": "^1.2.0",
    "config": "^1.12.0",
    "cookie-parser": "^1.3.4",
    "cyclejs": "^0.20.1",
    "express": "^4.12.3",
    "gulp-concat": "^2.5.2",
    "gulp-jshint": "^1.9.4",
    "gulp-jsx": "git://github.com/ivan-kleshnin/gulp-jsx.git",
    "gulp-less": "^3.0.2",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "jshint-stylish": "^1.0.1",
    "lodash": "^3.6.0",
    "lodash.range": "^3.0.0",
    "lodash.sortby": "^3.1.0",
    "lodash.values": "^3.0.0",
    "moment": "^2.9.0",
    "morgan": "^1.5.2",
    "node-uuid": "^1.4.3",
    "nunjucks": "^1.2.0",
    "require-directory": "^2.1.0",
    "winston": "^0.9.0",
    "winston-mail": "^0.3.2"
  },
  "devDependencies": {
    "babelify": "^5.0.4",
    "browserify": "^9.0.3",
    "glob": "^5.0.3",
    "gulp": "^3.8.11",
    "gulp-cached": "^1.0.4",
    "gulp-plumber": "^1.0.0",
    "mkdirp": "^0.5.0",
    "nodemon": "^1.3.7",
    "run-sequence": "^1.0.2",
    "watchify": "^3.0.0"
  },
  "frontendVendors": [
    "babel/polyfill",
    "classnames",
    "cyclejs",
    "lodash.sortby",
    "lodash.values",
    "node-uuid"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "experimental": true,
          "sourceMapRelative": "."
        }
      ]
    ]
  }
}
